<!--components/result/result.wxml-->

<scroll-view scroll-y="true" style="height: 1050rpx;">
    <t-divider content="原始图片" />
    <t-image src="{{'/image/ori.png'}}" width="100%" height="450rpx" shape="round" aria-label="ori" />
    <t-divider content="检测图片" />
    <t-image src="{{'/image/det.png'}}" width="100%" height="450rpx" shape="round" aria-label="ori" />
    <t-divider content="检测结果" />
